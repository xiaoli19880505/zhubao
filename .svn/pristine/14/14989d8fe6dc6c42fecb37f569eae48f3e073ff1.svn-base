        if(hao.length==0||shequ.length==0){
            bank.alertMessage("请填入社区和房号")
        }else{
            $('.over').show();
            $('#form').form('submit', {
                url:'<%=basePath%>sourceHouse/getReturnHouseFile',
                onSubmit: function (param) {

                    var isValid = $(this).form('validate');
                    if (!isValid) {
                        $('.over').hide();
                        bank.ajaxMessage('请检查是否有漏填或错填内容！');
                        return false;
                    }
                    return isValid;
                },
                success: function (data) {
                    // bank.ajaxMessage(data);
                    var a = $('<a href="<%=basePath %>'+data+'" target="_blank"></a>')[0];
                    var e = document.createEvent('MouseEvents');
                    e.initEvent('click', true, true);
                    a.dispatchEvent(e);
                    setTimeout(function () {
                        $('.over').hide();
                    },2000)
                },
                error: function (data) {
                    bank.alertMessage("数据库连接失败，请稍后再试");
                }
            }, 'json');

        }


