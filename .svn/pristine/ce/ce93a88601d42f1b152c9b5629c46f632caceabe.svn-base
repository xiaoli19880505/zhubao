	z-index: 99;
	line-height: 40px;
}

.button {
	padding-top: 24px;
}

.button .btn-login {
	width: 79%;
	height: 44px;
	line-height: 44px;
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
