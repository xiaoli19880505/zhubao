	-moz-osx-font-smoothing: grayscale;
}

#AllTabs .tabs-first .tabs-icon:before {
	content: "\e631";
}

#AllTabs .tabs-selected .tabs-icon {
	color: #f2c337;
}

#AllTabs .tabs-header {
	border: none;
	margin-top: 10px;
	background-color: #fff;
}

#AllTabs .tabs-header .tabs {
