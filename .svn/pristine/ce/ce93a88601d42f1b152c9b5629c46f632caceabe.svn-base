	font-weight: 700;
	font-style: normal;
	font-size: 28px;
	color: #FFFFFF;
}

#procsss .step .step-img {
	background: url(../img/applicationForm/grey.png);
	width: 35px;
	height: 35px;
	display: inline-block;
}

#procsss .step .step-img span {
	width: 35px;
	height: 35px;
	display: inline-block;
	line-height: 35px;
