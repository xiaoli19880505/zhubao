}


/*********审核流程**************/

.process .startEnd {
	height: 40px;
	width: 40px;
	border-radius: 50%;
	text-align: center;
	line-height: 40px;
	border: #797979 solid 1px;
}

.process .lineblock {
	display: inline-block;
}

.process .line {
	width: 50px;
	height: 1px;
	justify-content: center;
	background: #797979;
}

.process .node {
	height: 40px;
	padding: 0 15px;
	justify-content: center;
	border: #797979 solid 1px;
	line-height: 40px;
	cursor: pointer;
}

.addckprocess {
	padding: 20px 50px;
	display: flex;
	flex-direction: column;
	text-align: center;
	display: block;
	margin-top: 20px;
}

.addckprocess .quan {
	height: 40px;
	width: 40px;
	display: block;
	border-radius: 50%;
	text-align: center;
	line-height: 40px;
	border: #797979 solid 1px;
	margin: 0 auto 40px auto;
}

.addckprocess .fang {
	height: 40px;
	padding: 0 15px;
	display: block;
	justify-content: center;
	border: #797979 solid 1px;
	line-height: 40px;
	margin: 0 auto 40px auto;
	cursor: pointer;
}

.addckprocess .fang:hover {
	color: red;
	border: red solid 1px;
}

.addckprocess .line {
	width: 55px;
	height: 2px;
	display: block;
	justify-content: center;
	margin: 0 auto;
	position: relative;
	line-height: 3px;
	border-bottom: #797979 solid 1px;
}

.ckprocess {
	padding: 20px 50px;
	display: flex;
	flex-direction: row;
	flex-wrap: nowrap;
	overflow: hidden;
	width: 90%;
}

.ckprocess .quan {
	height: 40px;
	width: 40px;
	border-radius: 50%;
	text-align: center;
	line-height: 40px;
	border: #797979 solid 1px;
	float: left;
}

#processbox {
	display: flex;
	flex-direction: row;
	flex-wrap: nowrap;
	float: left;
}

.ckprocess .line,
#processbox .line {
	width: 50px;
	height: 2px;
	justify-content: center;
	background: #797979;
	margin-top: 20px;
	float: left;
}

.ckprocess .fanContainer {
	width: auto;
	float: left;
}

.fanContainer {
	position: relative;
	overflow: hidden;
}

.ckprocess .fang,
#processbox .fang {
	height: 40px;
	padding: 0 15px;
	text-align: center;
	justify-content: center;
	border: #797979 solid 1px;
	line-height: 40px;
	cursor: pointer;
	float: left;
	width: auto;
	min-width: 100px;
	max-width: 200px;
}

.fanclose {
	position: absolute;
	z-index: 1;
	width: 10px;
	top: 2px;
	right: 2px;
	cursor: pointer;
}

.ckprocess .line,
#processbox .line {
	width: 50px;
	height: 2px;
	justify-content: center;
	background: #797979;
	margin-top: 20px;
	float: left;
}

.ckprocess .quan {
	height: 40px;
	width: 40px;
	border-radius: 50%;
	text-align: center;
	line-height: 40px;
	border: #797979 solid 1px;
	float: left;
}

#lcattribute {
	width: 97.6%;
	margin: 15px 0 15px 0;
	border-left: #ddd solid 1px;
	border-top: #ddd solid 1px;
}

#flowPanel table,
#flowPanel table tr {
