@charset "utf-8";

/*===============================系统配置============================*/

* {
	padding: 0;
	margin: 0;
}

ul,
ol,
li {
	list-style: none;
}

a {
	text-decoration: none;
}

body {
	font: 14px/24px "microsoft yahei", "宋体", Arial, sans-serif, helvetica, Verdana, Tahoma;
	background: #fff;
}

i,
em {
	font-style: normal;
	font-size: 12px;
	margin-left: 5px;
	color: #fff;
}


/*==伪元素设置==*/

a:link,
a:visited,
a:active,
a:hover {
	text-decoration: none;
	outline: none;
}


/*==清除浮动==*/

.clear {
	clear: both;
	zoom: 1;
}

.clear:before,
.clear:after {
	content: "";
	display: block;
	clear: both;
}


/*===系统图标设置==*/

.icon {
	font-size: 16px;
	fill: currentColor;
	overflow: hidden;
}

.white {
	color: #fff !important;
}

.green {
	color: #399D46!important;
}

.red {
	color: #f00!important;
}

.blue {
	color: #169bd5;
}

.golden {
	color: rgb(250, 197, 70);
}

/**formatter样式**/
.ared{
	color: #FF0000;
	margin: 0 8px;
	cursor: default;
}
.agreen{
	color: #00978A;
	margin: 0 8px;
	cursor: pointer;
}
.ablue{
	color: #00a2d4;
	cursor: pointer;
	margin: 0 8px;
}
.aspan{
	float: right;
	color: #00a2d4;
	cursor: pointer;
	margin: 0 8px;
}
/*input框只读*/

.readonly .validatebox-readonly {
	background: #f2f2f2 !important;
}

.readonly input {
	cursor: no-drop!important;
}

.disable .validatebox-readonly {
	background: #f2f2f2 !important;
}

.disable input {
	cursor: no-drop!important;
}

.panel-fit .p5 {
	padding: 5px !important;
}

.h3 {
	height: 3px !important;
}

.h8 {
	height: 8px !important;
}

.title {
	width: 99%;
	background: #F2C337;
	padding-left: 1%;
	font-size: 14px;
	height: 32px;
	line-height: 32px;
}
input[type="checkbox"], input[type="radio"]{
	vertical-align: middle;
}

/*===============================项目公共部分设置============================*/


/*===顶部===*/

#AllRegion .northRegion {
	height: 70px;
	overflow: hidden;
	width: 100%;
	background: #006BA0;
}

#AllRegion .northRegion .north_left {
	float: left;
	height: 70px;
	line-height: 70px;
	color: #D8E8F0;
	width: 280px;
	padding-left: 30px;
	font-size: 25px;
}

#AllRegion .northRegion .north_center {
	float: left;
	height: 70px;
	line-height: 70px;
	text-align: center;
	font-size: 18px;
	color: #D8E8F0;
	width: calc(100% - 550px);
}

#AllRegion .northRegion .north_right {
	float: left;
	height: 70px;
	width: 240px;
}

#AllRegion .northRegion .north_right div {
	float: right;
	height: 70px;
	line-height: 50px;
	margin-right: 10px;
}

#AllRegion .northRegion .north_right div img {
	width: 30px;
	height: 30px;
	margin: 20px 10px;
	vertical-align: middle;
	background: #fff;
	display: inline-block;
	border-radius: 50%;
}

#AllRegion .northRegion .north_right div span {
	color: #fff;
	margin-right: 10px;
}

#AllRegion .northRegion a {
	font-size: 15px;
	width: 40px;
	height: 70px;
	line-height: 70px;
	text-align: center;
	display: inline-block;
	cursor: pointer;
}

