}

#loginPanel .form {
	position: absolute;
	left: 50%;
	top: 50%;
	height: 452px;
	width: 435px;
	border: 0;
	background-color: rgba(255, 255, 255, 0.5);
	margin: 0 auto;
	transform: translate(-50%, -50%);
	border-radius: 8px;
}

.header {
	height: 155px;
	width: 100%;
	position: relative;
	font-family: "Microsoft Yahei";
}

.header h1 {
	padding-top: 40px;
	padding-bottom: 44px;
	font-size: 24px;
	text-align: center;
	color: #005BAC;
}

.header .left {
	display: inline-block;
	width: 29%;
	height: 1px;
	border-top: solid 1px #D4AA44;
	position: relative;
