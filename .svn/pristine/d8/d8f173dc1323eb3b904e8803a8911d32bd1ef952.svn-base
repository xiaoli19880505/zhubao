package com.sys.pojo;

import java.util.List;

/**
 * 档案附件表，主要是图片
 */
public class Volelearc {
    private String elearcvolId;//档案附件id

    private String archiveid;//申请表id

    private String elearcname;//附件名称

    private Short pagenumber;//附件的数目（图片数）

    private Short ordinal;//序号，外来户从201开始

    private String flag;//不用的老数据，标记

    private Short thread;//不用的老数据，不明字段

    private String isshow;//不用的老数据，是否显示

    private List<VolelearcDtl> volelearcDtls;

    public String getElearcvolId() {
        return elearcvolId;
    }

    public void setElearcvolId(String elearcvolId) {
        this.elearcvolId = elearcvolId == null ? null : elearcvolId.trim();
    }

    public String getArchiveid() {
        return archiveid;
    }

    public void setArchiveid(String archiveid) {
        this.archiveid = archiveid == null ? null : archiveid.trim();
    }

    public String getElearcname() {
        return elearcname;
    }

    public void setElearcname(String elearcname) {
        this.elearcname = elearcname == null ? null : elearcname.trim();
    }

    public Short getPagenumber() {
        return pagenumber;
    }

    public void setPagenumber(Short pagenumber) {
        this.pagenumber = pagenumber;
    }

    public Short getOrdinal() {
        return ordinal;
    }

    public void setOrdinal(Short ordinal) {
        this.ordinal = ordinal;
    }

    public String getFlag() {
        return flag;
    }

    public void setFlag(String flag) {
        this.flag = flag == null ? null : flag.trim();
    }

    public Short getThread() {
        return thread;
    }

    public void setThread(Short thread) {
        this.thread = thread;
    }

    public String getIsshow() {
        return isshow;
    }

    public void setIsshow(String isshow) {
        this.isshow = isshow == null ? null : isshow.trim();
    }

    public List<VolelearcDtl> getVolelearcDtls() {
        return volelearcDtls;
    }

    public void setVolelearcDtls(List<VolelearcDtl> volelearcDtls) {
        this.volelearcDtls = volelearcDtls;
    }
}