/*放大，缩小，左旋转，右旋转，操作*/
.imgOperate{
	display: none;
	position:fixed;
	top:50%;
	left:50%;
	transform: translate(-50%, -50%);
	height:500px;
	width: 600px;
	background: #000;
	opacity: 0.8;
}
.imgOperate .optImg{
	  width: 401px;
	  height: 401px;
	  margin: 30px auto 0;
	  border: solid 1px #fff;
	  text-align: center;
	  overflow: auto;
	  box-sizing: border-box;
  }
.imgOperate .optImg  img{
    display:block;
	margin: 0 auto;
	vertical-align: middle;
	transition: All 0.4s ease-in-out;
	-webkit-transition: All 0.4s ease-in-out;
	-moz-transition: All 0.4s ease-in-out;
	-o-transition: All 0.4s ease-in-out;
}
.imgOperate .optColumn{
	width: 100%;
	height: 24px;
}
.imgOperate .optColumn span{
	display: inline-block;
	width: 22px;
	height: 22px;
	cursor: pointer;
}
.imgOperate .optColumn .leftRotate{
	background: url("../img/public/leftRoate.png") no-repeat 100% 100%;
}
.imgOperate .optColumn .leftRotate:hover{
	background: url("../img/public/leftRoate_hover.png") no-repeat  100% 100%;
}
.imgOperate .optColumn .rightRotate{
	background: url("../img/public/rightRoate.png") no-repeat  100% 100%;
}
.imgOperate .optColumn .rightRotate:hover{
	background: url("../img/public/rightRoate_hover.png") no-repeat  100% 100%;
}
.imgOperate .optColumn .expand{
	background: url("../img/public/expand.png") no-repeat  100% 100%;
}
.imgOperate .optColumn .expand:hover{
	background: url("../img/public/expand_hover.png") no-repeat  100% 100%;
}
.imgOperate .optColumn .shrink{
	background: url("../img/public/shrink.png") no-repeat  100% 100%;
}
.imgOperate .optColumn .shrink:hover{
	background: url("../img/public/shrink_hover.png") no-repeat  100% 100%;
}
.imgOperate .optColumn .close{
