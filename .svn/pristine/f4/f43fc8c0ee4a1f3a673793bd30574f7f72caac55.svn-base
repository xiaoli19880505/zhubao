}
#AllRegion .northRegion .north_center {
	float: left;
	height: 70px;
	line-height: 70px;
	text-align: center;
	font-size: 18px;
	color: #D8E8F0;
	width: calc(100% - 740px);
}
#AllRegion .northRegion .north_right {
	float: right;
	height: 70px;
	width:380px;
}

#AllRegion .northRegion .north_right div {
	float: right;
	height: 70px;
