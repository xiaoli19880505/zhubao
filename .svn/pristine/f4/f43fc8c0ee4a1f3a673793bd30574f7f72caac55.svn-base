/*================================忘记密码===============================*/
.pwdcontainer{
	width:900px;
	height:500px;
	margin: 120px auto 0;
	border: solid 1px #d3d3d3;
}
.pwdcontainer .pwdTitle{
	width: 100%;
	margin-top: 10px;
	overflow: hidden;
}
.pwdcontainer .pwdContent{
	width: 100%;
	overflow: hidden;
}
.pwdcontainer .titleItem{
	float: left;
	margin-right: 20px;
	cursor: pointer;
}
.pwdcontainer .arrow {
	width: 160px;
	height: 40px;
	text-align: center;
	color: #fff;
	line-height: 40px;
	background-color: #ff6767;
	position: relative;
}

.pwdcontainer .arrow:before {
	content:"";
	position: absolute;
	left: 0;
	top:0 ;
	border: dashed 20px transparent;
	border-left: 20px solid #fff;
}
.pwdcontainer .arrowOne:before {
	content:"";
	position: absolute;
	left: 0;
	top:0 ;
	border: 0;
	border-left:0;
}
.pwdcontainer .arrow:after {
	content:"";
	position: absolute;
	left:160px ;
	top: 0;
	border: dashed 20px transparent;
	border-left: 20px solid #ff6767;
}
.pwdcontainer .changeForm .formItem {
	margin-bottom: 40px !important;
}
.pwdcontainer .lightBlue{
	background-color:#87cefa !important;
}
.pwdcontainer .lightBlue:after {
	border-left: 20px solid #87cefa;
}
