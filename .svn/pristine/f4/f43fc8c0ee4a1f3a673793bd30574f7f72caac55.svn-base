	background: #3d3d3d;
	border: none;
}

.panel-header,
.panel-body {
	border-width: 0;
	border-style: solid;
}

.westRegion .accordion .accordion-body {
	background-color: #252525;
	border: none;
}

.accordion-header-selected .panel-title {
	color: #fff !important;
}

#westTree .tree-folder {
	background: none !important;
	margin-left: -15px;
}

#westTree .tree-expanded,
#westTree .tree-collapsed,
#westTree .tree-file {
	background: none!important;
}

#westTree .tree-node {
	background: #3d3d3d;
	-webkit-transition: all 2s;
	-moz-transition: all 2s;
	-ms-transition: all 2s;
	-o-transition: all 2s;
	transition: all 2s;
	padding-left: 6px;
	height: 32px;
	line-height: 32px;
}

#westTree ul .tree-node {
	background: #252525;
}

#westTree .tree-node a {
	display: inline-block;
	float: right;
	width: 16px;
	height: 16px;
	opacity: 0.6;
	filter: alpha(opacity=60);
	margin: 6px 16px 0 0;
}

#westTree .tree-node .tree-title {
	font-weight: 600;
}

#westTree ul .tree-node .tree-title {
	font-weight: 500;
}

#westTree .tree-node-selected,
#westTree .tree-node-hover {
	background: #3d3d3d;
	color: #b8c7ce;
}

#westTree ul .tree-node-selected,
#westTree ul .tree-node-hover {
	background: #252525;
	color: #b8c7ce;
}

#westTree ul .tree-node-selected {
	background: #0092DC;
}

#westTree .tree-node:hover {
	background: #3d3d3e;
	color: #b8c7ce;
}

#westTree ul .tree-node:hover {
	background: #0092DC;
}

#westTree ul .tree-node-hover .tree-title,
 ul .tree-node-selected .tree-title {
	color: #fff!important;
}
#westTree ul .tree-file {
	margin-top: 2px\0;
}
#westTree .tree-file:before,
#westTree ul .tree-folder:before {
	display: inline-block;
	content: "\e63e";
	position: relative;
	color: #fff !important;
	fill: currentColor;
	overflow: hidden;
	top: -5px;
	font-family: "iconfont" !important;
	font-size: 14px;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
}

#westTree .tree-title {
	color: #b8c7ce;
	font-weight: 500;
	font-size: 14px;
}

.accordion-collapse {
	margin-right: 10px !important;
	background: url("../img/public/accordion.gif") no-repeat -19px 0;
}

.accordion-expand {
	background: url("../img/public/accordion.gif") no-repeat 0 0;
}


/*定义滚动条样式（webkit内核可用）*/

.westRegion::-webkit-scrollbar {
	width: 15px !important;
}

.westRegion::-webkit-scrollbar-button {
	background-color: #252525 !important;
}

.westRegion::-webkit-scrollbar-corner {
	background-color: black !important;
}

.westRegion::-webkit-scrollbar-thumb {
	background-color: #4f4f4f !important;
}

.westRegion::-webkit-scrollbar-track {
	background-color: #252525 !important;
}


