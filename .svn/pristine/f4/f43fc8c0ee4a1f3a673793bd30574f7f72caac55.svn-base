/*********审核流程**************/
.addckprocess {
	padding: 20px 50px;
	display: flex;
	flex-direction: column;
	text-align: center;
	display: block;
	margin-top: 20px;
}
.addckprocess .quan {
	height: 40px;
	width: 40px;
	display: block;
	border-radius: 50%;
	behavior: url("../src/js/ie/PIE.htc");
	text-align: center;
	line-height: 40px;
	border: #797979 solid 1px;
	margin: 0 auto 40px auto;
}
.addckprocess .fang {
	height: 40px;
	padding: 0 15px;
	display: block;
	justify-content: center;
	border: #797979 solid 1px;
	line-height: 40px;
	margin: 0 auto 40px auto;
	cursor: pointer;
	box-sizing: border-box;
	min-width: 130px;
}

.addckprocess .fang:hover {
	color: red;
	border: red solid 1px;
}

.addckprocess .line {
	width: 55px;
	height: 2px;
	display: block;
	justify-content: center;
	margin: 0 auto;
	position: relative;
	line-height: 3px;
	border-bottom: #797979 solid 1px;
}

.ckprocess {
	padding: 20px 50px;
	display: flex;
	flex-direction: row;
	flex-wrap: nowrap;
	overflow-x: auto;
	overflow-y: hidden;
}

.ckprocess .quan {
	height: 40px;
	width: 40px;
	border-radius: 50%;
	behavior: url("../src/js/ie/PIE.htc");
	text-align: center;
	line-height: 40px;
	border: #797979 solid 1px;
    float: left;
}

#processbox {
	display: flex;
	flex-direction: row;
	flex-wrap: nowrap;
}

.ckprocess .line, #processbox .line {
	width: 50px;
	height: 2px;
	justify-content: center;
	background: #797979;
	margin-top: 20px;
	float: left;
}

.ckprocess .fanContainer {
	width: auto;
	float: left;
}

.fanContainer {
	position: relative;
	overflow: hidden;
}

.ckprocess .fang, #processbox .fang {
	height: 40px;
	padding: 0 15px;
	box-sizing: border-box;
	text-align: center;
	justify-content: center;
	border: #797979 solid 1px;
	line-height: 40px;
	cursor: pointer;
	float: left;
	width: auto;
	min-width: 130px;
/*	max-width: 200px;*/
}

.ckprocess .fangred {
	color: #ff0000;
	border: #ff0000 solid 1px !important;
}

.fanclose {
	position: absolute;
	z-index: 1;
	width: 10px;
	top: 2px;
	right: 2px;
	cursor: pointer;
}
.ckprocess .proStart {
	float: left;
	display: flex;
	flex-direction: row;
	flex-wrap: nowrap;
}
.ckprocess .proEnd {
	float: left;
	display: flex;
	flex-direction: row;
	flex-wrap: nowrap;
}
