	height: 70px;
	line-height: 70px;
	text-align: center;
	display: inline-block;
	cursor: pointer;
}

#AllRegion .northRegion a:hover {
	background: #000;
}

/*===左侧===*/

#AllRegion .westRegion {
	width: 200px;
	background: #252525;
}

#AllRegion .westRegion #leftNav {
	overflow: hidden;
}

#AllRegion .layout-panel-west {
	background: #252525;
}

#AllRegion .layout-expand-west {
